<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
 
  <servlet>
  	<servlet-name>IndexController</servlet-name>
  	<display-name>IndexController</display-name>
  	<description></description>
  	<servlet-class>com.neu.controllers.IndexController</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>CheckinController</servlet-name>
  	<display-name>CheckinController</display-name>
  	<description></description>
  	<servlet-class>com.neu.controllers.CheckinController</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>IndexController</servlet-name>
  	<url-pattern>/index</url-pattern>
  </servlet-mapping>
   <servlet-mapping>
  	<servlet-name>CheckinController</servlet-name>
  	<url-pattern>/CheckinController</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>CheckinController</servlet-name>
  	<url-pattern>/getcheckin</url-pattern>
  </servlet-mapping>
</web-app>
